<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.winter.app.notice.NoticeMapper">
  	<select id="getList">
  		SELECT
  			*
  		FROM
  			NOTICE
  		ORDER BY
  			BOARD_NUM DESC
  	
  	</select>
  	
  
  </mapper>